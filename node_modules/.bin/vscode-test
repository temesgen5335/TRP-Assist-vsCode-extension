#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/out/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/out/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules:/Users/temesgeng/10xTRP/week1/vsCode_extension/trp1-assist/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/out/bin.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/out/bin.mjs" "$@"
fi
